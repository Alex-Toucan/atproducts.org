---
import Head from '../layouts/Head.astro';
import Scripts from '../layouts/Scripts.astro';
import Hero   from '../components/Hero.astro';
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';
import BackToTop from '../components/BackToTop.astro';
import Accordion from '../components/accordion/Accordion.astro';
import AccordionItem from '../components/accordion/AccordionItem.astro';
---

<Head title="AT Products LLC" description="AT Products LLC is an Informational Technology Company, with computer resources" keywords="AT Products, Ethical Hacking Society, The Script Community, AT Products LLC, ATproducts" author="Alex Toucan" 
additionalContent="<meta name='google-site-verification' content='8CRfwExNu3kB77SccZhzqhwnETsU2dnm0CACBJSRJ5M'>"
bulletin={[
  { bulletinImage: "3YEAR.webp" },
  { bulletinImage: "220-1-101024.webp" },
  { bulletinImage: "220-2-101024.webp" },
  { bulletinImage: "220-3-101024.webp" },
  { bulletinImage: "220-4-101024.webp" },
]} />
<div id="page">
  <Navbar />
  <div id="main-content">
    <Hero
      id="main"
      title="AT Products LLC"
      description="The company that provides <strong>web solutions</strong> to your <strong>web problems</strong>!"
      marginBottom="false"
      buttons={[
        { href: "/paid", text: "Paid Services", type: "primary" },
      ]},
      dropdownItems={[
        { navigationDesc: "General Information", navigationLink: "#general-info" },
        { navigationDesc: "Products", navigationLink: "#products" },
        { navigationDesc: "Allies", navigationLink: "#allies" },
        { navigationDesc: "Credits", navigationLink: "#credits" },
        { navigationDesc: "History", navigationLink: "#history" },
        { navigationDesc: "What's New?", navigationLink: "#what-new" },
      ]},
      icons={[
        { icon: "github", desc: "GitHub", href: "https://github.com/Alex-Toucan/atproducts.xyz" },
        { icon: "twitter-x", desc: "Twitter / X", href: "https://x.com/ATProductsLLC" },
        { icon: "youtube", desc: "YouTube", href: "https://youtube.com/c/AlexToucanAT" },
        { icon: "envelope-fill", desc: "Contact Form / Email", href: "/form/" },
        { icon: "cloud-sun-fill", desc: "Weather", href: "https://x.com/ATPWeather" },
      ]},
    />
    <div id="carousel-A" class="carousel carousel-main carousel-fade slide shadow px-0 container-xsm mt-xmd-2 mb-3 pointer-event" data-bs-ride="carousel">
      <div class="pt-xmd-2 px-xmd-2">
        <div class="carousel-inner">
          <div class="carousel-item active" data-bs-interval="10000">
            <img src="/media/slideshow/3YEAR.webp" class="d-block w-100" alt="3 Year Anniversary">
          </div>
          <div class="carousel-item" data-bs-interval="10000">
            <img src="/media/slideshow/220-1-101024.webp" class="d-block w-100" alt="2.20 Release Slide 1">
          </div>
          <div class="carousel-item" data-bs-interval="10000">
            <img src="/media/slideshow/220-2-101024.webp" class="d-block w-100" alt="2.20 Release Slide 2">
