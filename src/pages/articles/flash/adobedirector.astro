<Head title="Adobe Director" icon="Adobe_Flash_Player_32.svg.png" description="Flash Documentation presents 'Adobe Director'!" author="AT Products LLC & Flash Documentation" />
<div id="page">
  <Navbar pageTitle="Adobe Director" 
    link={[
     { name: "Flash Documentation" , href: "/flash" },
    ]}
  />
