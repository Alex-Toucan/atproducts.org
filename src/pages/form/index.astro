---
import Navbar    from '../../components/Navbar.astro';
import Footer    from '../../components/Footer.astro';
import BackToTop from '../../components/BackToTop.astro';
---

<!DOCTYPE html>
<html class="fixedtopnavimage" lang="en">
  <head>
    <title>Contact Form</title>
    <link rel="stylesheet" href="/static/cookie.css">
    <link rel="stylesheet" href="/static/bootstrap.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.min.css">
    <link rel="shortcut icon" type="image/png" href="/media/favicon/favicon.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/media/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/media/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/media/favicon/favicon-16x16.png">
    <link rel="icon" type="image/png" href="/media/favicon/favicon.png">
    <link rel="manifest" href="/media/favicon/site.webmanifest">
    <link rel="mask-icon" href="/media/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="title" content="Contact Form">
    <meta name="description" content="Tired of emails clients? Use this contact form for AT Products LLC instead!">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="og:image" content="/media/favicon/iconembed.png">
        <meta name="msapplication-TileColor" content="#da532c">
    <script is:inline src="/static/darkmode.js"></script>
  </head>
  <body class="fixedtopnavimage">
      <div id="page">
<Navbar />
    <main id="form" class="container">
    <h1>Contact Form</h1>
    <p>If you don't wanna email us on your email client, use this instead. We'll reach out to your email provided.</p>
<form
  name="contact"
  action="/form/success"
  method="POST"
  data-netlify="true"
class>
  <div class="mb-3">
  <label for="exampleFormControlInput1" class="form-label">Email address</label>
  <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
</div>
<div class="mb-3">
  <label for="nameInput" class="form-label">Name</label>
  <input name="name" class="form-control" id="nameInput" placeholder="John Doe">
</div>
<div class="mb-3">
  <label for="textareaInput" class="form-label">Example textarea</label>
  <textarea class="form-control" id="textareaInput" rows="3"></textarea>
</div>
</form>
</main>
<Footer />
<BackToTop />
</div>
    <script is:inline src="https://cdn.jsdelivr.net/npm/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script is:inline src="https://cdn.jsdelivr.net/npm/jquery/dist/jquery.min.js"></script>
     <script is:inline src="https://cdn.jsdelivr.net/npm/jquery-ui/dist/jquery-ui.min.js"></script>
    <script is:inline src="/static/scroll.js"></script>
    <script is:inline src="/static/purecookie.js"></script>
    <script is:inline src="/static/enabler.js"></script>
    </body>
    </html>
