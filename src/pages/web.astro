---
import CardPage  from '../layouts/CardPage.astro';
import Navbar    from '../components/Navbar.astro';
import Footer    from '../components/Footer.astro';
import BackToTop from '../components/BackToTop.astro';
---

<!DOCTYPE html>
<html class="fixedtopnavimage" lang="en">
  <head>
    <title>Web Projects</title>
    <link rel="stylesheet" href="/static/cookie.css">
    <link rel="stylesheet" href="/static/bootstrap.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.min.css">
    <link rel="apple-touch-icon" sizes="180x180" href="/media/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/media/favicon/android-chrome-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/media/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/media/favicon/favicon-16x16.png">
    <link rel="manifest" href="/media/favicon/site.webmanifest">
            <link rel="preload" as="style" href="/static/bootstrap.css">
    <link rel="preload" as="image" href="/media/images/hero-web.webp">
    <link rel="mask-icon" href="/media/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    <meta charset="UTF-8">
    <meta name="title" content="Web Projects">
    <meta name="description" content="View our web projects.">
    <meta name="keywords" content="AT Products, Ethical Hacking Soeciety, The Script Community, AT Products LLC, ATproducts">
    <meta name="author" content="Alex Toucan">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="og:image" content="/media/favicon/iconembed.png">
        <meta name="msapplication-TileColor" content="#da532c">
    <script is:inline src="/static/darkmode.js"></script>
  </head>
  <body class="fixedtopnavimage">
  <div id="page">
<Navbar />
        <main id="web" class="text-center"> 
        <CardPage id="web" title="Web Projects" 
        cardLinks={[
         { cardTitle: "Flash Documentation", cardDescription: "Here are some things about Adobe Flash!", cardImage: "/media/images/flashdoc.webp", cardHref: "/flash", secondaryHref: "/backup/flash" },
         { cardTitle: "MDickie Projects", cardDescription: "This is a fanmade site about what has MDickie developed since the 1990s!", cardImage: "/media/images/mdickiepro.webp", cardHref: "/mdickie", additionalImageClasses: "p-1" },
         ]} />
        </main>
<Footer hr="none" />
<BackToTop />
</div>
    <script is:inline src="https://cdn.jsdelivr.net/npm/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script is:inline src="https://cdn.jsdelivr.net/npm/jquery/dist/jquery.min.js"></script>
     <script is:inline src="https://cdn.jsdelivr.net/npm/jquery-ui/dist/jquery-ui.min.js"></script>
    <script is:inline src="/static/scroll.js"></script>
    <script is:inline src="/static/purecookie.js"></script>
    <script is:inline src="/static/enabler.js"></script>
    </body>
    </html>
