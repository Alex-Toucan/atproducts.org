                <div class="tab-link">
                  <a href="/pynx" class="btn btn-primary w-100">Visit</a>
                </div>
              </div>
              <div class="products-6 tab-pane fade" id="products-6" role="tabpanel" aria-labelledby="products-6-tab" tabindex="0">
